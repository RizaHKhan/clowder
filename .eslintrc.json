{
    "env": {
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-recommended",
        "prettier"
    ],
    "rules": {
        "vue/multi-word-component-names": [
            "warn",
            {
                "ignores": [
                    "Cabin",
                    "Cargo",
                    "Login",
                    "Register",
                    "Dashboard",
                    "Sidebar",
                    "Header",
                    "Footer",
                    "Settings",
                    "Airline",
                    "Inventory",
                    "Pricing",
                    "Branding"
                ]
            }
        ]
    }
}
